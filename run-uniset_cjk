#!/bin/sh
# $Revision: 1.4 $ */
this=${0##*/}
what=$(ident $this | awk '{printf "%s", $2}')
printf "  /* generated by $this $what */\n"
uniset +WIDTH-A -cat=Me -cat=Mn -cat=Cf \
   +E000..F8FF \
   +F0000..FFFFD \
   +100000..10FFFD  c |\
awk '(NR==1){sub("{","static const struct interval ambiguous[] = {"); }{sub("^","  ");print;}'
