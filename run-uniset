this=${0##*/}
what=$(ident "$this" | awk '{printf "%s", $3}')
printf "  /* generated by %s %s */\n" "$this" "$what"
uniset +cat=Me +cat=Mn \
	+0600-0605 +061C +06DD +070F \
	+1160-11FF \
	+D7B0-D7C6 \
	+D7CB-D7FB \
	c | \
awk '(NR==1){sub("{","static const struct interval combining[] = {"); }{sub("^","  ");print;}'
