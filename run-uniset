#!/bin/sh
# $Revision: 1.5 $ */
this=${0##*/}
what=$(ident $this | awk '{printf "%s", $2}')
printf "  /* generated by $this $what */\n"
uniset +cat=Me +cat=Mn +cat=Cf \
	-00AD \
	+1160-11FF \
	+200B c | \
awk '(NR==1){sub("{","static const struct interval combining[] = {"); }{sub("^","  ");print;}'
